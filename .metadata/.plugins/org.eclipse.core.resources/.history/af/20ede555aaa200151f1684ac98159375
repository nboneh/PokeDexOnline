	         Class.forName("org.postgresql.Driver");
	         c = DriverManager
	            .getConnection("postgres://bsooqzlgrnlzpw:_OeBjMpdVS7A-u6vDmBNpDhgEl@ec2-54-83-59-203.compute-1.amazonaws.com:5432/dab3gubtq4co9o?ssl=true",
	            "bsooqzlgrnlzpw", " _OeBjMpdVS7A-u6vDmBNpDhgEl");
	      } catch (Exception e) {
	         e.printStackTrace();
