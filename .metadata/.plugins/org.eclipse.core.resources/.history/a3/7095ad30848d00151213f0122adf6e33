package com.Boneh.Nir.Quiz4.client;

import org.springframework.context.ApplicationContext;
import org.springframework.context.support.ClassPathXmlApplicationContext;

import com.Boneh.Nir.Quiz4.model.Book;

public class SpringBook4Driver {
	
	public static void main(String[] args){
		ApplicationContext context = 
	     new ClassPathXmlApplicationContext("Boneh_spring.xml");

	      Book bookYouReadIn = (Book) context.getBean("book");
	      
	      System.out.println(bookYouReadIn);

	 	}

}
